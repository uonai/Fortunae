{
  "name": "fortunae",
  "version": "1.0.0",
  "description": "A minimal personal finance application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/uonai/Fortunae",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^22.6.1",
    "electron-packager": "^14.2.1",
    "eslint": "^7.1.0"
  },
  "dependencies": {
    "d3": "^5.16.0",
    "electron-reload": "^1.5.0",
    "electron-store": "^5.1.1",
    "electron-window-manager": "^1.0.6",
    "fs": "0.0.1-security",
    "fs-extra": "^9.0.0",
    "ipc": "0.0.1",
    "save": "^2.4.0",
    "v8-compile-cache": "^2.1.0"
  },
  "build": {
    "productName": "fortunae",
    "appId": "fortunae",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "asar": false
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.ico",
      "asar": false
    }
  }
}
